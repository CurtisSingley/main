            "cwd": "/mnt/c/Users/cls88/OneDrive/Code/455/Lab11",
            "environment": [],
            "externalConsole": true,
            "pipeTransport": {
                "debuggerPath": "/usr/bin/gdb",
                "pipeProgram": "c:\\windows\\system32\\bash.exe",
